      "settings": {
        "foreground": "#66BB6A"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#EF5350"
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed"
