{
  "name": "Beryl Dark",
  "type": "dark",
  "colors": {
    // base
    "focusBorder": "#EEEEEE",
    "foreground": "#FAFAFA",
    "disabledForeground": "#F5F5F5",
    "widget.shadow": "#616161",
    "selection.background": "#616161",
    "descriptionForeground": "#FAFAFA",
    "errorForeground": "#FF1744",
    "icon.foreground": "#FAFAFA",
    "sash.hoverBorder": "#F5F5F5",

    // action
    "toolbar.hoverBackground": "#424242",
    "toolbar.activeBackground": "#424242",
    "toolbar.hoverOutline": "#616161",

    // button
    "button.background": "#424242",
    "button.foreground": "#FAFAFA",
    "button.border": "#424242",
    "button.hoverBackground": "#616161",
    "button.secondaryBackground": "#212121",
    "button.secondaryForeground": "#FAFAFA",
    "button.secondaryHoverBackground": "#616161",
    "checkbox.background": "#424242",
    "checkbox.foreground": "#FAFAFA",
    "checkbox.border": "#F5F5F5",

    // activity bar
    "activityBar.background": "#212121",
    "activityBar.activeBackground": "#424242",
    "activityBar.foreground": "#FAFAFA",
    "activityBar.inactiveForeground": "#F5F5F5",
    "activityBar.activeFocusBorder": "#EEEEEE",
    "activityBarBadge.background": "#EEEEEE",
    "activityBarBadge.foreground": "#424242",

    // side bar
    "sideBar.background": "#212121",
    "sideBar.foreground": "#FAFAFA",
    "sideBar.dropBackground": "#21212180",
    "sideBarTitle.foreground": "#FAFAFA",
    "sideBarSectionHeader.background": "#424242",
    "sideBarSectionHeader.foreground": "#FAFAFA",

    // minimap
    "minimap.background": "#212121",
    "minimap.findMatchHighlight": "#FAFAFA80",
    "minimap.selectionHighlight": "#F5F5F580",
    "minimap.errorHighlight": "#FF174480",
    "minimap.warningHighlight": "#FFC40080",
    "minimap.selectionOccurrenceHighlight": "#F5F5F580",

    // editor
    "editor.background": "#212121",
    "editor.foreground": "#FAFAFA",
    "editorLineNumber.foreground": "#757575",
    "editorLineNumber.activeForeground": "#E0E0E0",
    "editorCursor.foreground": "#EEEEEE",
    "editor.selectionBackground": "#616161AA",
    "editor.selectionHighlightBorder": "#FAFAFA48",
    "editor.lineHighlightBackground": "#424242",
    "editorIndentGuide.background": "#424242",
    "editorIndentGuide.activeBackground": "#9E9E9E",
    "editorRuler.foreground": "#9E9E9E",
    "editorLightBulb.foreground": "#FFC400",
    "editorLightBulbAutoFix.foreground": "#FFC400",
    "editorBracketMatch.border": "#F5F5F580",
    "editorBracketHighlight.foreground1": "#FF5252",
    "editorBracketHighlight.foreground2": "#FF6E40",
    "editorBracketHighlight.foreground3": "#FFFF00",
    "editorBracketHighlight.foreground4": "#B2FF59",
    "editorBracketHighlight.foreground5": "#64FFDA",
    "editorBracketHighlight.foreground6": "#448AFF"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#757575"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#FFA726"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#FAFAFA"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#EF5350"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#FF5252"
      }
    },
    {
      "name": "Storage Modifier",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FF7043"
      }
    },
    {
      "name": "Storage Type",
      "scope": [
        "storage.type",
        "keyword.interface",
        "keyword.struct"
      ],
      "settings": {
        "foreground": "#42A5F5"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#FF5252"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#FF7043"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#00C853"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "meta.block variable.other"
      ],
      "settings": {
        "foreground": "#FFA726"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#F5F5F5"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#FFC107"
      }
    },
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#AB47BC"
      }
    },
    {
      "name": "Function Argument",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFA726"
      }
    },
    {
      "name": "Embedded",
      "scope": [
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#FF5252"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.quoted punctuation"
      ],
      "settings": {
        "foreground": "#00C853"
      }
    },
    {
      "name": "Template String",
      "scope": [
        "string.template",
        "string.template punctuation"
      ],
      "settings": {
        "foreground": "#BDBDBD"
      }
    },
    {
      "name": "Symbols, Inherited Class, Markup Heading",
      "scope": [
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#EF5350"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#66BB6A"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "support.type"
      ],
      "settings": {
        "foreground": "#FFC107"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#42A5F5"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#00C853"
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFA726"
      }
    },
    {
      "name": "entity.name.method.js, meta.method.js",
      "scope": [
        "entity.name.method.js",
        "entity.name.method.jsx",
        "entity.name.method.ts",
        "entity.name.method.tsx",
        "meta.class-method.js entity.name.function.js",
        "meta.class-method.jsx entity.name.function.jsx",
        "meta.class-method.ts entity.name.function.ts",
        "meta.class-method.tsx entity.name.function.tsx",
        "variable.function.constructor"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#42A5F5"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#66BB6A"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
